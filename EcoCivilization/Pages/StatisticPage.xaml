<Page x:Class="EcoCivilization.Pages.StatisticPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EcoCivilization.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StatisticPage">

    <Grid>
        <WrapPanel Name="wpFoto">
            <Image x:Name="imgFirst"
                   Source="{Binding Photo1}" 
                   Width="100"
                   Height="100"/>
            <Image x:Name="imgSecond"
                   Source="{Binding Photo2}"
                   Width="100"
                   Height="100"/>
            <Image x:Name="imgThirst"
                   Source="{Binding Photo3}"
                   Width="100"
                   Height="100"/>
            <Image x:Name="imgFourth"
                   Source="{Binding Photo4}"
                   Width="100"
                   Height="100"/>
            <Image x:Name="imgFive"
                   Source="{Binding Photo5}"
                   Width="100"
                   Height="100"/>
        </WrapPanel>
    </Grid>
</Page>
